{
  "name": "email.message",
  "type": "object",
  "description": "Defines an email message.",
  "inputs": {
    "from": {
      "type": "string"
    },
    "to": {
      "type": "string"
    },
    "cc": {
      "type": "string"
    },
    "bcc": {
      "type": "string"
    },
    "subject": {
      "type": "string",
      "maxConnectors": 1
    },
    "text": {
      "type": "string",
      "maxConnectors": 1
    },
    "html": {
      "type": "string",
      "maxConnectors": 1
    }
  },
  "outputs": {
    "message": {
      "type": "email.message"
    }
  }
}
